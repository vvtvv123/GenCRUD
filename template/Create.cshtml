@using {{ project }}.Web.Startup
@model {{ project }}.Web.Models.{{ model }}s.{{ model }}ViewModel

@{
    ViewBag.CurrentPageName = PageNames.{{ model }}s;
}

<h2>Create</h2>

<h4>Create{{ model }}ViewModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            {% for k,v in vars.items() %}
            <div class="form-group">
                <label asp-for="{{ k }}" class="control-label"></label>
                <input asp-for="{{ k }}" class="form-control" />
                <span asp-validation-for="{{ k }}" class="text-danger"></span>
            </div>
            {% endfor %}
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

